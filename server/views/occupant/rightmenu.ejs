<div class="filterbar menu-card" data-id="occupants-menu">
    <!-- MOBILE MENU -->
    <div class="visible-xs">
        <div class="btn-toolbar clearfix" role="toolbar">
            <div class="btn-group pull-left" role="group">
                <button type="button" class="btn btn-default user-action" data-id="list-action-add-occupant" role="button"><div><i class="fa fa-plus-circle"></i></div></button>
            </div>
            <div class="btn-group pull-right" role="group">
                <button type="button" class="btn btn-default default-filter user-action" data-id="list-filter" data-toggle="button" role="button"><%= t('All') %> <div class="all-filter-label text-muted"></div></button>
                <button type="button" class="btn btn-default user-action" data-id="list-filter" data-value="<%= t('Lease in progress') %>" data-toggle="button" role="button"><%= t('Lease in progress') %><div class="all-active-filter-label text-muted"></div></button>
                <button type="button" class="btn btn-default user-action" data-id="list-filter" data-value="<%= t('Lease terminated') %>" data-toggle="button" role="button"><%= t('Lease terminated') %><div class="all-inactive-filter-label text-muted"></div></button>
            </div>
        </div>
    </div>

    <div class="hidden-xs">
        <button type="button" class="btn btn-default btn-block user-action" data-id="list-action-add-occupant" role="button"><i class="fa fa-plus-circle"></i> <%= t('New') %></button>
        <h4><%= t('Status') %></h4>
        <button type="button" class="btn btn-default btn-block default-filter user-action" data-id="list-filter" data-toggle="button" role="button"><%= t('All') %> <span class="all-filter-label text-muted"></span></button>
        <button type="button" class="btn btn-default btn-block user-action" data-id="list-filter" data-value="<%= t('Lease in progress') %>" data-toggle="button" role="button"><%= t('Lease in progress') %> <span class="all-active-filter-label text-muted"></span></button>
        <button type="button" class="btn btn-default btn-block user-action" data-id="list-filter" data-value="<%= t('Lease terminated') %>" data-toggle="button" role="button"><%= t('Lease terminated') %> <span class="all-inactive-filter-label text-muted"></span></button>
    </div>
</div>

<div class="menu-card" data-id="occupants-selection-menu">
    <div class="visible-xs">
        <div class="btn-toolbar clearfix" role="toolbar">
            <div class="btn-group pull-left" role="group">
                <button type="button" data-id="list-action-edit-occupant" class="btn btn-default user-action only-mono-selection" role="button"><i class="fa fa-pencil-square-o"></i></button>
                <button type="button" data-id="list-action-manage-documents" class="btn btn-default user-action only-mono-selection" role="button"><i class="fa fa-folder-o"></i></button>
                <button type="button" data-id="list-action-rents-history" class="btn btn-default user-action only-mono-selection" role="button"><i class="fa fa-history"></i></button>
                <!--<button type="button" data-id="list-action-print" class="btn btn-default user-action" role="button"><i class="fa fa-print"></i></button>-->
            </div>
            <div class="btn-group pull-right" role="group">
                <button type="button" class="btn btn-default user-action cancel-selection" role="button"><i class="fa fa-reply"></i></button>
            </div>
        </div>
    </div>
    <div class="hidden-xs">
        <button type="button" class="btn btn-default btn-block user-action cancel-selection" role="button"><i class="fa fa-reply"></i> <%= t('Cancel selection') %></button>
        <button type="button" data-id="list-action-edit-occupant" class="btn btn-default btn-block user-action only-mono-selection" role="button"><i class="fa fa-pencil-square-o"></i> <%= t('Lease contract') %></button>
        <button type="button" data-id="list-action-manage-documents" class="btn btn-default btn-block user-action only-mono-selection" role="button"><i class="fa fa-folder-o"></i> <%= t('Manage documents') %></button>
        <button type="button" data-id="list-action-rents-history" class="btn btn-default btn-block user-action only-mono-selection" role="button"><i class="fa fa-history"></i> <%= t('Timeline rents') %></button>
        <button type="button" data-id="list-action-print" class="btn btn-default btn-block user-action" role="button"><i class="fa fa-print"></i> <%= t('Print') %></button>
        <h4 class="list-selection-menu-label"></h4>
        <div class="list-selected-elements list-group list-group-selection"></div>
    </div>
</div>

<div class="menu-card" data-id="occupant-form-menu" data-target="#occupant-form">
    <div class="visible-xs">
        <div class="btn-toolbar clearfix" role="toolbar">
            <div class="btn-group pull-left" role="group">
                <button type="button" data-id="list-action-save-form" type="button" class="btn btn-default user-action" role="button"><i class="fa fa-save"></i></button>
            </div>
            <div class="btn-group pull-right" role="group">
                <button type="button" class="btn btn-default user-action cancel-form" role="button"><i class="fa fa-reply"></i></button>
            </div>
            <div class="btn-group pull-right" role="group">
                <button type="button" data-id="list-action-remove-occupant" class="btn btn-warning user-action" role="button"><i class="fa fa-trash"></i></button>
            </div>
        </div>
    </div>
    <div class="hidden-xs">
        <button type="button" class="btn btn-default btn-block user-action cancel-form" role="button"><i class="fa fa-reply"></i> <%= t('Cancel') %></button>
        <button type="button" data-id="list-action-save-form" type="button" class="btn btn-default btn-block user-action" role="button"><i class="fa fa-save"></i> <%= t('Save') %></button>
        <button type="button" data-id="list-action-remove-occupant" class="btn btn-warning btn-block user-action" role="button"><i class="fa fa-trash"></i> <%= t('Remove') %></button>
        <h4 class=""><%= t('Rent information') %></h4>
        <div class="row">
            <div class="col-xs-6"><%= t('Deposit') %></div>
            <div id="occupant-form-summary-guaranty" class="col-xs-6 text-right">-</div>
        </div>
        <br>
        <div class="row">
            <div class="col-xs-6"><%= t('Property') %><small><a href="#" title="<%= t('Including Common Area Maintenance fees Excluding VAT') %>"> (<%= t('ICAMEVAT') %>)</a></small></div>
            <div id="occupant-form-summary-rentwithexpenses" class="col-xs-6 text-right">-</div>
        </div>
        <div class="row">
            <div class="col-xs-6"><%= t('Discount') %><small><a href="#" title="<%= t('Excluding VAT') %>"> (<%= t('EVAT') %>)</a></small></div>
            <div id="occupant-form-summary-discount" class="col-xs-6 text-right">-</div>
        </div>
        <br class="occupant-form-vat-row">
        <div class="occupant-form-vat-row row">
            <div class="col-xs-6"><%= t('Rent') %><small><a href="#" title="<%= t('Including Common Area Maintenance fees Excluding VAT') %>"> (<%= t('ICAMEVAT') %>)</a></small></div>
            <div id="occupant-form-summary-total-rentwithexpenses"class="col-xs-6 text-right">-</div>
        </div>
        <div class="occupant-form-vat-row row">
            <div class="col-xs-6"><%= t('VAT') %></div>
            <div id="occupant-form-summary-vat" class="col-xs-6 text-right">-</div>
        </div>
        <br>
        <div class="row">
            <div class="col-xs-6"><%= t('Rent') %><small><a href="#" title="<%= t('Including Common Area Maintenance fees Including VAT') %>"> (<%= t('ICAMIVAT') %>)</a></small></div>
            <div id="occupant-form-summary-totla-rentwithexpensesandvat"class="col-xs-6 text-right">-</div>
        </div>
    </div>
</div>

