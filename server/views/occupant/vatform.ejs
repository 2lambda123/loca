 <div class="form-card">
    <div id="rent-vat-form" class="grid-form">
        <div class="form-action">
            <h1>Taux de TVA</h1>
            <input type="button" id="rentvatform-send" value="Enregistrer">
        </div>
        <input type="hidden" id="rent._id" value="<%= occupant._id %>" />
        <input type="hidden" id="rent.month" value="<%= rent.month %>" disabled/>
        <input type="hidden" id="rent.year" value="<%= rent.year %>" disabled/>
        <fieldset>
            <div data-row-span="5">
                <!--
                <div data-field-span="2">
                    <label>Ã  partir de</label>
                  	<input id="rent.vatStartTime" type="text" value="<%= frMonthLabels[rent.month-1]+" "+rent.year %>" disabled/>  
                    <span class="my-error-tip"></span>
                </div>
                -->
                <div data-field-span="5">
                    <label>Taux de T.V.A. en %</label>
        			<input type="text" id="rent.vatRatio" value="<%=  rent.vatRatio?Number(rent.vatRatio*100).toFixed(2):'' %>"/>
                    <span class="my-error-tip"></span>
                </div>
            </div>
        </fieldset>

        <span class="errormsg"></span>                    
    </div>
</div>