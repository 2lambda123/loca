<meta http-equiv="content-language" content="fr-FR">
<meta lang="fr" content="<%=webSiteInfo.metatags.description%>" name="description"/>
<meta lang="fr" content="<%=webSiteInfo.metatags.keywords%>" name="Keywords"/>
<meta lang="fr" content="<%=webSiteInfo.metatags.location%>" name="Location"/>
<meta lang="fr" content="<%=webSiteInfo.metatags.rating%>" name="Rating"/>
<meta lang="fr" content="<%=webSiteInfo.metatags.author.name%>" name="author"/>

<!-- Open Graph Meta Tags -->
<meta property="og:type" content="<%=webSiteInfo.metatags.type%>"/>
<meta property="og:title" content="<%=webSiteInfo.metatags.title%>"/>
<meta property="og:description" content="<%=webSiteInfo.metatags.description%>"/>
<meta property="og:site_name" content="<%=webSiteInfo.product.name%>"/>
<meta property="og:url" content="<%=webSiteInfo.product.website%>"/>
<meta property="og:image" content="<%=webSiteInfo.product.imageUrl%>"/>
<!--<meta property="fb:admins" content=""/>-->

<!-- Twitter Card Meta Tags -->
<meta name="twitter:card" content="<%=webSiteInfo.metatags.title%>"/>
<meta name="twitter:title" content="<%=webSiteInfo.metatags.title%>"/>
<meta name="twitter:description" content="<%=webSiteInfo.metatags.description%>"/>
<meta name="twitter:image" content="<%=webSiteInfo.product.imageUrl%>"/>
<!--<meta name="twitter:site" content=""/>-->
<meta name="twitter:creator" content="<%=webSiteInfo.metatags.author.twitter%>"/>