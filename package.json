{
  "name": "loca",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run buildprod && NODE_ENV=production node server.js",
    "buildprod": "NODE_ENV=production babel-node scripts/build.js",
    "dev": "npm run builddev && npm run watch",
    "watch": "parallelshell 'npm run watch:img' 'npm run watch:less' 'npm run watch:front:js' 'npm run watch:back:js'",
    "watch:front": "parallelshell 'npm run watch:img' 'npm run watch:less' 'npm run watch:front:js'",
    "watch:back:js": "babel-watch -w backend server.js",
    "watch:front:js": "babel-watch -x -e [.js,.mjs] -w frontend/lib -w frontend/view scripts/build.js js",
    "watch:less": "babel-watch -x -e [.less] -w frontend/less scripts/build.js css",
    "watch:img": "babel-watch -x -e [.png,.jpg] -w frontend/images scripts/build.js img",
    "builddev": "babel-node scripts/build.js static",
    "lint": "eslint .",
    "test-only": "babel-node node_modules/mocha/bin/_mocha",
    "test": "npm run lint && babel-node node_modules/babel-istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha",
    "mongodump": "node scripts/mongodump.js",
    "mongorestore": "node scripts/mongorestore.js"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "bcryptjs": "^2.4.0",
    "body-parser": "^1.16.0",
    "bootbox": "^4.4.0",
    "bootstrap": "^3.3.7",
    "bootstrap-datepicker": "^1.6.4",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^1.2.0",
    "ejs": "^2.5.5",
    "errorhandler": "^1.5.0",
    "express": "^4.0.0",
    "express-winston": "^2.1.0",
    "frontexpress": "^1.0.1",
    "handlebars": "^4.0.5",
    "historyjs": "^1.8.0-b2",
    "i18next": "^6.0.3",
    "i18next-browser-languagedetector": "^1.0.0",
    "i18next-express-middleware": "^1.0.2",
    "i18next-localstorage-cache": "^0.3.0",
    "i18next-node-fs-backend": "^0.1.3",
    "i18next-sprintf-postprocessor": "^0.2.2",
    "i18next-xhr-backend": "^1.1.0",
    "jquery": "^3.1.1",
    "jquery-validation": "^1.15.1",
    "mathjs": "^3.9.0",
    "method-override": "2",
    "minivents": "^2.0.2",
    "moment": "^2.17.1",
    "mongobackup": "^0.2.1",
    "mongojs": "^2.4.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "serve-favicon": "^2.3.2",
    "string": "^3.3.3",
    "sugar": "^2.0.1",
    "traceur": "0.0.111",
    "traceur-source-maps": "^1.0.6",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-istanbul": "^0.12.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-watch": "^2.0.5",
    "cssnano": "^3.7.6",
    "eslint": "^3.7.0",
    "fs-extra": "^2.0.0",
    "imagemin": "^5.2.2",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.0",
    "less": "^2.7.1",
    "live-reload": "^1.1.0",
    "mocha": "^3.2.0",
    "parallelshell": "^2.0.0",
    "proxyquire": "^1.7.11",
    "purify-css": "^1.1.9",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-includepaths": "^0.1.8",
    "rollup-plugin-uglify": "^1.0.1",
    "sinon": "^1.17.7",
    "supertest": "^3.0.0"
  }
}
